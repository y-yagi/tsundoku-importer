{
  "name": "Tsundoku Importer",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Importer for tundoku",
  "icons": {
    "128": "images/icon128.png"
  },
  "permissions": [
    "tabs",
    "contextMenus",
    "clipboardRead",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": ["background.js"]
  }
}
